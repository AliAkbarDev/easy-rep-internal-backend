  "author": "HTML5SOLUTIONS",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.5.0",
    "@sentry/profiling-node": "^10.5.0",
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "docker": "^1.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
